<% layout('../layout') -%>
    <link rel="stylesheet" href="/static/admin/assets/css/lib/datatable/dataTables.bootstrap.min.css" />
    <div class="content mt-3">
        <div class="animated fadeIn">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <strong class="card-title">All IEN Members</strong>
                        </div>
                        <div class="card-body">
                            <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>First Name</th>
                                        <th>Last Name</th>
                                        <th>Email</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (i=0;i<teamMembers.length;i++) { %>
                                        <tr>
                                            <td>
                                                <%= teamMembers[i]['firstName']%>
                                            </td>
                                            <td>
                                                <%= teamMembers[i]['lastName']%>
                                            </td>
                                            <td>
                                                <%= teamMembers[i]['email']%>
                                            </td>
                                            <td>
                                                <a href="./members/<%=teamMembers[i]['id']%>/edit"><span class="ti-pencil-alt"></span
                                        ></a>
                                            </td>
                                        </tr>
                                        <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="/static/admin/assets/js/lib/data-table/datatables.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/dataTables.bootstrap.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/dataTables.buttons.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/buttons.bootstrap.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/jszip.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/pdfmake.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/vfs_fonts.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/buttons.html5.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/buttons.print.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/buttons.colVis.min.js"></script>
    <script src="/static/admin/assets/js/lib/data-table/datatables-init.js"></script>