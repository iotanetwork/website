<div id="team" class="team cta-team-outer">
    <div class="container">
        <div class="row text-center">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <div class="cta-team-inner">
                    <h1 class="wow fadeInDown" data-wow-delay="0.1s">
                        Our Awesome Team
                    </h1>
                    <div class="at-grid" data-column="3" data-wow-delay="0.2s">
                        <% for (let i = 0; i < teamMembers.length; i++) { %>
                            <div class="at-column">
                                <div class="at-user">
                                    <div class="at-user__avatar">
                                        <img src="<%= teamMembers[i]['picture'] %>" />
                                    </div>
                                    <div class="at-user__name">
                                        <%= teamMembers[i]['firstName'] %>
                                            <%=
                                    teamMembers[i]['lastName'] %>
                                    </div>
                                    <ul class="at-social">
                                        <% if(teamMembers[i]['socialHandles']) { for (let s = 0; s < Object.keys(teamMembers[i]['socialHandles']).length; s++) { if(teamMembers[i]['socialHandles'][Object.keys(teamMembers[i]['socialHandles'])[s]].length > 0){ if(Object.keys(teamMembers[i]['socialHandles'])[s] == 'discord') { %>
                                            <li class="at-social__item">
                                                <a href="<%= socialAccounts[Object.keys(teamMembers[i]['socialHandles'])[s]]['baseUrl'] %>"><i
                                                    class="fa <%= socialAccounts[Object.keys(teamMembers[i]['socialHandles'])[s]]['iconClass'] %>"
                                                    aria-hidden="true">
                                                    @<%= teamMembers[i]['socialHandles'][Object.keys(teamMembers[i]['socialHandles'])[s]] %></i></a>
                                            </li>
                                            <% } else { %>
                                                <li class="at-social__item">
                                                    <a href="<%= socialAccounts[Object.keys(teamMembers[i]['socialHandles'])[s]]['baseUrl'] %><%= teamMembers[i]['socialHandles'][Object.keys(teamMembers[i]['socialHandles'])[s]] %>">
                                                <i class="fa <%= socialAccounts[Object.keys(teamMembers[i]['socialHandles'])[s]]['iconClass'] %>"
                                                    aria-hidden="true"></i>
                                            </a>
                                                </li>
                                                <% }}}} %>
                                    </ul>
                                </div>
                            </div>
                            <% } %>
                    </div>
                    <a class="btn btn-action btn-gplus wow fadeInDown" href="/team"><span></span> <span>See All</span></a
                    >
                </div>
            </div>
        </div>
    </div>
</div>